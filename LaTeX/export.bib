@article{zhonghua,
    author = {Li, Zhonghua and Dang, Leining and Yang, Yanguang and Li, Zhihui},
    title = "{Study of hybrid NS-DSMC simulation method with chemical non-equilibrium for transitional hypersonic flow}",
